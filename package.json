{
  "name": "rxjs-extra",
  "version": "2.0.0",
  "description": "Collection of extra operators for RxJS 5",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "clean_cjs": "rm -rf dist/cjs",
    "clean_es5": "rm -rf dist/es5",
    "clean_types": "rm -rf dist/typings",
    "clean_tests2png": "rm -rf .rxjs-repo docs_app",
    "bundle_es5": "npm run build_es5 && rollup -c && npm run closure_compile_es5",
    "closure_compile_es5": "mkdir -p dist/es5/bundles && node tools/run-closure-compiler.js",
    "build_es5": "npm run clean_es5 && tsc -p tsconfig.es5.json",
    "build_cjs": "npm run clean_cjs && tsc -p tsconfig.cjs.json",
    "build_types": "npm run clean_types && tsc -p tsconfig.types.json",
    "test": "TS_NODE_PROJECT=tsconfig.spec.json mocha --opts spec/mocha.opts \"spec/**/a-spec.ts\"",
    "tests2png": "TS_NODE_PROJECT=tsconfig.spec.json mocha --opts spec/tests2png.opts \"spec/**/a-spec.ts\"",
    "tests2png_full": "npm run prepare_tests2png && npm run tests2png && npm run move_generated_png && npm run clean_tests2png",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts' 'demo/**/*.js'",
    "lint_fix": "npm run lint -- --fix",
    "clone_rxjs_repo": "git clone https://github.com/ReactiveX/rxjs.git --depth=1 .rxjs-repo",
    "create_tests2png_dir": "mkdir -p ./docs_app/content/img",
    "prepare_tests2png": "npm run clean_tests2png && npm run clone_rxjs_repo && npm run create_tests2png_dir",
    "move_generated_png": "cp -R ./docs_app/content/img/ ./doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martinsik/rxjs-extra.git"
  },
  "keywords": [
    "Rx",
    "RxJS",
    "ReactiveX",
    "ReactiveExtensions",
    "Streams",
    "Observables",
    "Observable",
    "Stream"
  ],
  "author": "Martin Sikora",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martinsik/rxjs-extra/issues"
  },
  "homepage": "https://github.com/martinsik/rxjs-extra#readme",
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/lodash": "^4.14.53",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.5",
    "@types/sinon": "^1.16.35",
    "@types/sinon-chai": "^2.7.27",
    "chai": "^3.5.0",
    "color": "^1.0.3",
    "escape-string-regexp": "^1.0.5",
    "gm": "^1.23.0",
    "google-closure-compiler-js": "^20170521.0.0",
    "lodash": "^4.17.4",
    "mocha": "^5.2.0",
    "normalize-newline-cli": "^1.1.0",
    "rollup": "^0.41.6",
    "rxjs-compat": "^6.4.0",
    "sinon-chai": "^2.8.0",
    "source-map-support": "^0.4.11",
    "symbol-observable": "^1.0.4",
    "ts-node": "^8.0.2",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "rxjs": "^6.0.0"
  },
  "peerDependencies": {
    "rxjs": "^6.0.0"
  },
  "typings": "./dist/package/RxJS-Extra.d.ts"
}
